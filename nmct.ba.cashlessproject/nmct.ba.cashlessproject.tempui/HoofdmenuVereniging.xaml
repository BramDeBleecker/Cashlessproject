        <Grid.RowDefinitions>
            <RowDefinition Height="51*"/>
            <RowDefinition Height="34*"/>
            <RowDefinition Height="484*"/>
        </Grid.RowDefinitions>
        <Grid HorizontalAlignment="Left" Height="34" Grid.Row="1" VerticalAlignment="Top" Width="792" Grid.ColumnSpan="2" Margin="0,20,0,0" Grid.RowSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="132" Height="34"/>
