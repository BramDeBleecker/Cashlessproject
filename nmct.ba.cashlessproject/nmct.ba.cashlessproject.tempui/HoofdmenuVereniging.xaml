        <Label x:Name="lblHoofdmenukassaMedewerker" Content="medewerker" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="Ubuntu" Width="182" Grid.ColumnSpan="2" Margin="607,0,0,0"/>
