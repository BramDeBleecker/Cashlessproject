        <TextBox x:Name="TxtHoofdmenukassaMedewerker" HorizontalAlignment="Left" Height="23" Margin="607,24,0,0" TextWrapping="Wrap" Text="naam" VerticalAlignment="Top" Width="182" FontFamily="Ubuntu" VerticalContentAlignment="Center" Grid.ColumnSpan="2"/>
        <Label x:Name="lblHoofdmenukassaTitel" Content="Kassanaam" HorizontalAlignment="Left" Margin="10.5,10,0,0" VerticalAlignment="Top" Width="590" Height="49" FontFamily="Ubuntu" FontSize="36" Grid.RowSpan="2"/>

    </Grid>
